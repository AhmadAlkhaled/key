{
  "version": 2,
  "name": "maps-meet",
  "builds": [
    {
      "src": "client/src/index.html",
      "use": "parcel build",
      "config": {
        "publicUrl": "./static/"
      }
    },
    {
      "src": "Backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api(/.*)?",
      "dest": "Backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "client/src/index.html"
    }
  ]
}
